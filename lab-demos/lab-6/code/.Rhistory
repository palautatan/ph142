w <- sapply(1:100000, function(x) {
n          <- 20
sample     <- rpois(n=n, lambda=1)
lambda_hat <- mean(sample)
se_hat     <- sqrt(mean(sample)/n)
wi         <- (lambda_hat - 1) / se_hat
wi
}
)
